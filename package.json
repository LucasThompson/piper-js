{
  "name": "feats",
  "version": "0.0.1",
  "description": "Feature extraction library, written in TypeScript.",
  "main": "dist/ts/feats.js",
  "typings": "dist/ts/feats.d.ts",
  "scripts": {
    "test": "mocha test/*Test.ts --require ts-node/register",
    "timings": "mocha perf/*.ts --require ts-node/register",
    "profile": "npm run timings && node --prof-process $(ls -1tr isolate*.log | tail -1) > profile.txt",
    "build-browser": "tsc && browserify dist/ts/feats.js  -t babelify --standalone Feats -o ./dist/feats.js",
    "build": "tsc",
    "prepublish": "npm run build-browser",
    "postinstall": "npm run build-browser"
  },
  "bin": {
    "feats-bundle": "bin/build-module.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/LTDSP/feats.git"
  },
  "keywords": [
    "feature-extraction",
    "audio",
    "web-audio",
    "features",
    "mir"
  ],
  "author": "Lucas Thompson",
  "license": "GPL-2.0",
  "homepage": "https://bitbucket.org/LTDSP/feats#readme",
  "dependencies": {
    "@types/base64-js": "^1.1.4",
    "babelify": "^7.3.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "base64-js": "^1.2.0",
    "browserify": "^13.1.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-insert": "^0.5.0",
    "typescript": "^2.0.3",
    "vinyl-source-stream": "^1.1.0",
    "yargs": "^5.0.0"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/chai-as-promised": "0.0.29",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.41",
    "babel-cli": "^6.14.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "mocha": "^3.1.0",
    "ts-node": "^1.3.0",
    "tv4": "^1.2.7"
  }
}
